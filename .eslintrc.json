{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": [2, "double", { "avoidEscape": true }],
        "react/jsx-one-expression-per-line": "off"
    }
}
